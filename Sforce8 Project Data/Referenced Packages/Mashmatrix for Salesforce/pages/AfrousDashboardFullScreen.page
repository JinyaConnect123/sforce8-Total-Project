<apex:page showHeader="false"
           standardStylesheets="false"
           contentType="text/html"
           cache="true"
><apex:outputText escape="false" value="{!"<!DOCTYPE html PUBLIC \"-//W3C//DTD HTML 4.01 Transitional//EN\">"}"
/><html>
<head>
<meta http-equiv="X-UA-Compatible" content="IE=EmulateIE8" />
<style type="text/css">
html { 
  height:100%;
}
body { 
  height:100%; 
  margin:0;
  padding:0;
}
body span, body div {
  height:100%;
}
</style>
</head>
<body style="margin:0">
  <c:dashboard targetId="{!$CurrentPage.parameters.targetId}"
               contextId="{!$CurrentPage.parameters.id}"
               staticView="{!$CurrentPage.parameters.staticView}"
               hideMenubar="{!$CurrentPage.parameters.hideMenubar}"
               hideDrawer="{!$CurrentPage.parameters.hideDrawer}"
               collapseDrawer="{!$CurrentPage.parameters.collapseDrawer}"
               hideNavigator="{!$CurrentPage.parameters.hideNavigator}"
               hideDock="{!$CurrentPage.parameters.hideDock}"
  />
